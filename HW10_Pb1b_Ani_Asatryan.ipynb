        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x = np.array([-3, 1, 0, 7])\n",
        "y = np.array([2, 4, -1, 3])\n",
        "n = 1\n",
        "\n",
        "coef = np.polyfit(x, y, n)\n",
        "yy = np.polyval(coef, x)\n",
        "plt.plot(x, yy)\n",
        "plt.plot(x, y, \"o\")"
      ]
